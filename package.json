{
  "name": "transactions",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:standalone": "react-native start --port 9000",
    "test": "jest",
    "build": "npm run bundle:ios && npm run bundle:android",
    "bundle:ios": "react-native bundle --platform ios --entry-file index.js --dev false",
    "bundle:android": "react-native bundle --platform android --entry-file index.js --dev false",
    "serve-remote-assets:android": "adb reverse tcp:9999 tcp:9999 && npm run http-server -p 9000 build/outputs/android/remotes",
    "update:firebase": "node -r dotenv/config updateFirebase.js"
  },
  "dependencies": {
    "@react-native-vector-icons/material-icons": "^12.3.0",
    "@react-native/new-app-screen": "0.80.0",
    "dotenv": "^17.2.1",
    "firebase-admin": "^13.4.0",
    "react": "19.1.0",
    "react-native": "0.80.0",
    "react-native-safe-area-context": "^5.5.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@callstack/repack": "^5.1.0-rc.0",
    "@module-federation/enhanced": "^0.18.3",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@rspack/core": "^1.3.4",
    "@swc/helpers": "^0.5.17",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "http-server": "^14.1.1",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=18"
  }
}
